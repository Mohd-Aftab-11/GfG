import java.util.*;

class Solution {
    public String firstNonRepeating(String s) {
        int n = s.length();
        char[] result = new char[n];
        int[] freq = new int[26];
        Queue<Character> queue = new LinkedList<>();
        
        for (int i = 0; i < n; i++) {
            char c = s.charAt(i);
            freq[c - 'a']++;
            
            queue.offer(c);
            
            while (!queue.isEmpty() && freq[queue.peek() - 'a'] > 1) {
                queue.poll();
            }
            
            if (queue.isEmpty()) {
                result[i] = '#';
            } else {
                result[i] = queue.peek();
            }
        }
        
        return new String(result);
    }
    
    public static void main(String[] args) {
        Solution solution = new Solution();
        
        System.out.println(solution.firstNonRepeating("aabc"));
        System.out.println(solution.firstNonRepeating("bb"));
        System.out.println(solution.firstNonRepeating("abc"));
        System.out.println(solution.firstNonRepeating("aba"));
    }
}
